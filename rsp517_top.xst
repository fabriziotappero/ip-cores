set -tmpdir "./xst/tmp"
set -xsthdpdir "./xst"
run
-ifn rsp517_top.prj
-ifmt mixed
-ofn rsp517_top
-ofmt NGC
-p xc4vlx160-10-ff1148
-top rsp517_top
