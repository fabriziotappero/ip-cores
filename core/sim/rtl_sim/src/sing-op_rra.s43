.include "pmem_defs.asm"

.global main

main:
        /* -------------- TEST INSTRUCTION IN WORD MODE ------------------- */


        # Addressing mode: Rn
	#------------------------

	mov     #0x0001, r2        ;# Test 1
	mov     #0x7332, r4
	rra          r4            ;# RRA ({r4=0x7332}  => {r4=0x3999, C=0})
	mov          r2, r5

	mov     #0x0001, r2        ;# Test 2
	mov     #0x7333, r6
