.include "pmem_defs.asm"

.global main

main:
        /* -------------- TEST INSTRUCTION IN WORD MODE ------------------- */


        # Addressing mode: Rn
	#------------------------

	mov     #0x0000, r2        ;# Test 1
	mov     #0x3332, r4
	rrc          r4            ;# RRC ({C=0, r4=0x3332}  => {r4=0x1999, C=0})
	mov          r2, r5

	mov     #0x0000, r2        ;# Test 2
	mov     #0x3333, r6
	rrc          r6            ;# RRC ({C=0, r6=0x3333}  => {r6=0x1999, C=1})
	mov          r2, r7

	mov     #0x0001, r2        ;# Test 3
	mov     #0x3332, r8
