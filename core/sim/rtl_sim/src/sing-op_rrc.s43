        /* ----------------------- CLEAR MEMORY --------------------------- */
	mov     #0x0015, r4
	mov   #DMEM_200, r5
clear_mem_loop:
	clr    0(r5)
	incd     r5
	dec      r4
	jnz     clear_mem_loop

	mov     #0x7000, r15


