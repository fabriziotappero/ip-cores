	mov          r2, r9

	mov     #0x0001, r2        ;# Test 4
	mov     #0x3333, r10
	rrc         r10            ;# RRC ({C=1, r10=0x3333} => {r10=0x9999, C=1})
	mov          r2, r11

	mov     #0x1000, r15


