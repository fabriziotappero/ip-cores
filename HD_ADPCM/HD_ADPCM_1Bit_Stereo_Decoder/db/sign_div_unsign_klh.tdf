FUNCTION alt_u_div_e2f (denominator[6..0], numerator[19..0])
RETURNS ( quotient[19..0], remainder[6..0]);

--synthesis_resources = lut 147 
SUBDESIGN sign_div_unsign_klh
( 
	denominator[6..0]	:	input;
	numerator[19..0]	:	input;
	quotient[19..0]	:	output;
	remainder[6..0]	:	output;
) 
VARIABLE 
	divider : alt_u_div_e2f;
	norm_num[19..0]	: WIRE;
	protect_quotient[19..0]	: WIRE;
	protect_remainder[6..0]	: WIRE;

BEGIN 
	divider.denominator[] = denominator[];
	divider.numerator[] = norm_num[];
	norm_num[] = numerator[];
	protect_quotient[] = divider.quotient[];
	protect_remainder[] = divider.remainder[];
	quotient[] = protect_quotient[];
	remainder[] = protect_remainder[];
END;
